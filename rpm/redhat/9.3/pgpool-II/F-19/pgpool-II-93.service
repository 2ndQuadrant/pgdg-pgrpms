[Unit]
Description=PGPool-II Middleware Between PostgreSQL Servers And PostgreSQL Database Clients
After=syslog.target network.target

[Service]
EnvironmentFile=-/etc/sysconfig/pgpool-II-93
ExecStart=/usr/pgpool-9.3/bin/pgpool -f /etc/pgpool-II/pgpool.conf $OPTS 

[Install]
WantedBy=multi-user.target
